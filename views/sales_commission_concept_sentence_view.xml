<?xml version="1.0" encoding="utf-8" ?>

<odoo>

	<record model="ir.ui.view" id="sales_commission.concept_sentence_list">
      <field name="name">Listado de sentencias</field>
      <field name="model">sales_commission.concept_sentence</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
        </tree>
      </field>
    </record>

	<record id="action_sales_commission_concept_sentence" model="ir.actions.act_window">
		<field name="name">Sentencias de conceptos</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">sales_commission.concept_sentence</field>
		<field name="view_mode">tree,form</field>
	</record>

	<menuitem 
    id="sales_commission_concept_sentence_menu"
    parent="sales_commission_concepts_menu"
    name="Sentencias"
    action="action_sales_commission_concept_sentence"
    sequence="10"/>

</odoo>